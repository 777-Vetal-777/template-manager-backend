<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- The properties below are used in order to prevent default mapping BLOB to OID in case of PostgreSQL. -->
    <property name="blob" value="bytea" dbms="postgresql"/>
    <property name="blob" value="blob" dbms="all"/>

    <include file="releases/db.changelog-0.0.1.xml" relativeToChangelogFile="true"/>
    <include file="releases/db.changelog-0.0.2.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>